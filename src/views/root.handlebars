<div id="container">
    <div id="header">
        Routes
    </div>

    <div id="sidebar">
        <ul>
            <li>
            {{#each categorised}}
                <ul>
                    <li class="header">{{@key}}</li>
                    {{#each this}}
                    <li class="route-link">
                        <a href="#endpoint-{{method}};{{path}}">{{path}}</a>
                    </li>
                    {{/each}}
                </ul>
            {{/each}}
            </li>
        </ul>
    </div>

    <div id="content">
        {{#each routes}}
            <div class="route">
                <a name="endpoint-{{method}};{{path}}"></a>
                <h1 class="endpoint">{{method}} {{fullQuery}}</h1>
                <p class="endpoint-description">{{settings.description}}</p>
                <ul class="tags">
                    {{#each settings.tags}}
                    <li class="tag">{{this}}</li>
                    {{/each}}
                </ul>
                {{#if settings.validate.params}}
                <div class="validator-list parameters">
                    <h1>URI Components</h1>
                    <table class="pure-table uri-table">
                        <thead>
                            <tr>
                                <td>Parameter</td>
                                <td>Type</td>
                                <td>Description</td>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each settings.validate.params._inner.children}}
                            <tr>
                                <td class="key">{{key}}</td>
                                <td class="type">{{schema._type}}</td>
                                <td class="description">{{schema._description}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
                {{/if}}
                {{#if settings.validate.query}}
                <div class="validator-list parameters">
                    <h1>Query String</h1>
                    <table class="pure-table query-table">
                        <thead>
                            <tr>
                                <td>Key</td>
                                <td>Type</td>
                                <td>Description</td>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each settings.validate.query._inner.children}}
                            <tr>
                                <td class="key">{{key}}</td>
                                <td class="type">{{schema._type}}</td>
                                <td class="description">{{schema._description}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
                {{/if}}
                {{#if settings.validate.payload}}
                <div class="validator-list payload">
                    <h1>JSON Payload</h1>
                    <table class="pure-table payload-table">
                        <thead>
                            <tr>
                                <td>Attribute</td>
                                <td>Type</td>
                                <td>Description</td>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each settings.validate.payload._inner.children}}
                            <tr>
                                <td class="key">{{key}}</td>
                                <td class="type">{{schema._type}}</td>
                                <td class="description">{{schema._description}}</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
                {{/if}}
            </div>
        {{/each}}
    </div>

    <div id="footer">
        Base: <a href="{{baseUrl}}">{{baseUrl}}</a>
    </div>
</div>

